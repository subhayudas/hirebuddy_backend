                return !1;
        for (o = i; o-- !== 0; ) {
            const c = a[o];
            if (!(c === "_owner" && s.$$typeof) && !deepEqual(s[c], e[c]))
                return !1
        }
        return !0
    }
    return s !== s && e !== e
}
