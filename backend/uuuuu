function createContextScope(s, e=[]) {
    let i = [];
    function o(c, d) {
        const h = reactExports.createContext(d)
          , g = i.length;
        i = [...i, d];
        const b = $ => {
            var st;
            const {scope: _e, children: tt, ...rt} = $
              , et = ((st = _e == null ? void 0 : _e[s]) == null ? void 0 : st[g]) || h
              , it = reactExports.useMemo( () => rt, Object.values(rt));
