            return;
        const a = this.node.animationState.setActive("exit", !e);
        i && !e && a.then( () => {
            i(this.id)
        }
        )
    }
    mount() {
        const {register: e, onExitComplete: i} = this.node.presenceContext || {};
        i && i(this.id),
        e && (this.unmount = e(this.id))
    }
    unmount() {}
}
