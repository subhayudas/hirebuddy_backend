                }
                _setEventListener(is, rs, fs, Ns, Cs) {
                    fs.includes("mouse") ? is.addEventListener(fs, Ls => {
                        var Os;
                        (Os = this.linkService.eventBus) == null || Os.dispatch("dispatcheventinsandbox", {
                            source: this,
