  , ResumeImport = ({onResumeImported: s, showCreateFromScratch: e=!0}) => {
    const [i,o] = reactExports.useState(!1)
      , [a,c] = reactExports.useState(!1)
      , [d,h] = reactExports.useState(0)
      , [g,b] = reactExports.useState(null)
      , _ = useNavigate()
      , $ = [{
        label: "Uploading file",
