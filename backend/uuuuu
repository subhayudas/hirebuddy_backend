            function ht(Nt, jt, xt, Et) {
                const Pt = Nt.length;
                for (let St = 3; St < Pt; St += 4) {
                    const Bt = Nt[St];
                    if (Bt === 0)
                        Nt[St - 3] = jt,
                        Nt[St - 2] = xt,
                        Nt[St - 1] = Et;
