                        }),
                        Ls.lastChild.classList.add("richText", "popupContent");
                    else {
                        const ar = this._formatContents(Ws);
                        Ls.append(ar)
                    }
                    let Qs = !!mt(this, ms)
                      , Gs = Qs ? mt(this, ms) : mt(this, ks);
                    for (const ar of mt(this, Lt))
                        if (!Gs || o.Util.intersect(ar.data.rect, Gs) !== null) {
                            Gs = ar.data.rect,
                            Qs = !0;
                            break
