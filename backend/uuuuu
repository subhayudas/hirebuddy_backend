                setRotation(is, rs=this.container) {
                    if (!this.data.rect)
                        return;
                    const {pageWidth: fs, pageHeight: Ns} = this.parent.viewport.rawDims
                      , {width: Cs, height: Ls} = _e(this.data.rect);
