                    (jt = mt(this, _)) == null || jt.classList.toggle("small", bt < kt.SMALL_EDITOR_SIZE || Tt < kt.SMALL_EDITOR_SIZE)
                }
                fixDims() {
                    const {style: bt} = this.div
                      , {height: Tt, width: Yt} = bt
                      , Nt = Yt.endsWith("%")
                      , jt = !mt(this, tt) && Tt.endsWith("%");
                    if (Nt && jt)
                        return;
