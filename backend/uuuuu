                data: i.summary,
                onUpdate: xt => ss("summary", xt)
            });
        case "experience":
            return jsxRuntimeExports.jsx(EnhancedExperienceSection, {
                data: i.experience,
                onUpdate: xt => ss("experience", xt)
            });
