    }
    ),
    e
}
function handleAndDispatchCustomEvent(s, e, i, {discrete: o}) {
    const a = i.originalEvent.currentTarget
      , c = new CustomEvent(s,{
        bubbles: !0,
        cancelable: !0,
        detail: i
    });
