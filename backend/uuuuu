            _i = function(bt, Tt, Yt) {
                switch (Yt) {
                case 90:
                    return [Tt, -bt];
                case 180:
                    return [-bt, -Tt];
                case 270:
                    return [-Tt, bt];
                default:
                    return [bt, Tt]
                }
            }
            ,
            eo = function(bt) {
                switch (bt) {
                case 90:
                    {
                        const [Tt,Yt] = this.pageDimensions;
                        return [0, -Tt / Yt, Yt / Tt, 0]
                    }
                case 180:
                    return [-1, 0, 0, -1];
                case 270:
                    {
                        const [Tt,Yt] = this.pageDimensions;
                        return [0, Tt / Yt, -Yt / Tt, 0]
                    }
                default:
                    return [1, 0, 0, 1]
                }
            }
