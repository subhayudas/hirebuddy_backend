                        const os = 255 - Bt;
                        Nt[St - 3] = Nt[St - 3] * Bt + jt * os >> 8,
                        Nt[St - 2] = Nt[St - 2] * Bt + xt * os >> 8,
                        Nt[St - 1] = Nt[St - 1] * Bt + Et * os >> 8
                    }
                }
            }
            function pt(Nt, jt, xt) {
