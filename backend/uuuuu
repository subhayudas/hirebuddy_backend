        once: !0
    }),
    o ? dispatchDiscreteCustomEvent(a, c) : a.dispatchEvent(c)
}
var isDeltaInDirection = (s, e, i=0) => {
    const o = Math.abs(s.x)
      , a = Math.abs(s.y)
      , c = o > a;
    return e === "left" || e === "right" ? c && o > i : !c && a > i
}
