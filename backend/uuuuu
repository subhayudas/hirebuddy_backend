        }
    }
    isConnected() {
        return this.connectionState() === CONNECTION_STATE.Open
    }
    channel(e, i={
        config: {}
    }) {
        const o = `realtime:${e}`
          , a = this.getChannels().find(c => c.topic === o);
        if (a)
            return a;
        {
            const c = new RealtimeChannel(`realtime:${e}`,i,this);
