                }
                render() {
                    if (mt(this, bs))
                        return;
                    const {page: {view: is}, viewport: {rawDims: {pageWidth: rs, pageHeight: fs, pageX: Ns, pageY: Cs}}} = mt(this, ts)
                      , Ls = us(this, bs, document.createElement("div"));
