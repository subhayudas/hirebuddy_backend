                }
                _pumpOperatorList({renderingIntent: pt, cacheKey: gt, annotationStorageSerializable: wt}) {
                    const {map: yt, transfers: Rt} = wt
                      , ss = this._transport.messageHandler.sendWithStream("GetOperatorList", {
                        pageIndex: this._pageIndex,
