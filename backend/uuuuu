var xr, wn, $o, MutationCache = ($o = class extends Subscribable {
    constructor(e={}) {
        super();
        ps(this, xr);
        ps(this, wn);
        this.config = e,
        us(this, xr, new Map),
        us(this, wn, Date.now())
    }
    build(e, i, o) {
        const a = new Mutation({
            mutationCache: this,
            mutationId: ++kr(this, wn)._,
            options: e.defaultMutationOptions(i),
            state: o
        });
        return this.add(a),
        a
    }
    add(e) {
