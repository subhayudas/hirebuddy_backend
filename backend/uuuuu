                initial: {
                    opacity: 0,
                    y: 20
                },
                animate: {
                    opacity: 1,
                    y: 0
                },
                transition: {
                    delay: h * .1
                },
                children: jsxRuntimeExports.jsx(Card, {
                    className: "border border-gray-200 hover:border-amber-300 transition-colors",
                    children: jsxRuntimeExports.jsxs(CardContent, {
                        className: "p-4",
