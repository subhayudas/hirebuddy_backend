                }
                _bindJSAction(rs, fs) {
                    rs.href = this.linkService.getAnchorUrl("");
                    const Ns = new Map([["Action", "onclick"], ["Mouse Up", "onmouseup"], ["Mouse Down", "onmousedown"]]);
                    for (const Cs of Object.keys(fs.actions)) {
                        const Ls = Ns.get(Cs);
                        Ls && (rs[Ls] = () => {
                            var Os;
