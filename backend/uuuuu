            Zn = function() {
                const [bt,Tt] = this.parentDimensions
                  , {_borderLineWidth: Yt} = kt
                  , Nt = Yt / bt
                  , jt = Yt / Tt;
                switch (this.rotation) {
                case 90:
                    return [-Nt, jt];
                case 180:
                    return [Nt, jt];
                case 270:
                    return [Nt, -jt];
                default:
                    return [-Nt, -jt]
                }
            }
