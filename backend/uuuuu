function calculateProfileCompletion(s, e=[]) {
    if (!s)
        return 0;
    const i = [{
        key: "full_name",
        weight: 15
    }, {
        key: "location",
        weight: 10
    }, {
        key: "phone",
        weight: 10
    }, {
        key: "bio",
        weight: 20
    }, {
        key: "skills",
        weight: 15
    }, {
        key: "college",
        weight: 10
    }, {
        key: "linkedin",
        weight: 5
    }, {
        key: "github",
        weight: 5
    }, {
        key: "website",
        weight: 5
    }, {
        key: "resume_url",
        weight: 15
    }];
    let o = 0;
    const a = i.reduce( (d, h) => d + h.weight, 0);
    i.forEach(d => {
        const h = s[d.key];
        h && (Array.isArray(h) && h.length > 0 || typeof h == "string" && h.trim().length > 0 || typeof h == "boolean" || typeof h == "number") && (o += d.weight)
    }
    ),
    e && e.length > 0 && (o += 10);
    const c = Math.round(o / (a + 10) * 100);
    return !s.resume_url && c >= 80 ? 80 : c
}
