      , h = interpolate(d, e, {
        ease: Array.isArray(a) ? a : defaultEasing(e, a)
    });
    return {
        calculatedDuration: s,
        next: g => (c.value = h(g),
        c.done = g >= s,
        c)
    }
}
const isNotNull$1 = s => s !== null;
