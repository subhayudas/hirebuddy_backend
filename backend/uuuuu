                        }), jsxRuntimeExports.jsx("div", {
                            className: "h-4 bg-gray-200 rounded w-28 animate-pulse"
                        })]
                    }), jsxRuntimeExports.jsx("div", {
                        className: "h-8 bg-gray-200 rounded w-20 animate-pulse"
                    })]
                })
            }), jsxRuntimeExports.jsx(CardContent, {
                className: "space-y-4",
