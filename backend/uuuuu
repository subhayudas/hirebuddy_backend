            x: 0,
            width: 0
        };
        return nt - lt.x - lt.width / 2
    }
    )
      , et = useTransform(rt, [-i, 0, i], [40, e, 40])
      , it = useSpring(et, {
        mass: .1,
        stiffness: 150,
        damping: 12
    })
