                        children: [jsxRuntimeExports.jsx(Target, {
                            className: "h-4 w-4 text-blue-600 flex-shrink-0"
                        }), jsxRuntimeExports.jsx("span", {
                            className: "truncate",
                            children: "Top Skill Recommendations"
                        })]
                    }), b.length > 15 && jsxRuntimeExports.jsx(Button, {
                        variant: "ghost",
                        size: "sm",
                        onClick: at,
