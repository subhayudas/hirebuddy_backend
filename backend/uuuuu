              , d = e[e.length - 2];
            if ((c.x - d.x) * (a.y - d.y) >= (c.y - d.y) * (a.x - d.x))
                e.pop();
            else
                break
        }
        e.push(a)
    }
    e.pop();
    const i = [];
    for (let o = s.length - 1; o >= 0; o--) {
        const a = s[o];
        for (; i.length >= 2; ) {
