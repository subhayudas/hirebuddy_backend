                        ar.classList.add("popupDate"),
                        mt(this, ds).then(Rr => {
                            ar.textContent = Rr
                        }
                        ),
                        Os.append(ar)
                    }
                    const Ws = mt(this, xs)
                      , Bs = mt(this, Ts);
                    if (Bs != null && Bs.str && (!(Ws != null && Ws.str) || Ws.str === Bs.str))
                        g.XfaLayer.render({
                            xfaHtml: Bs.html,
                            intent: "richText",
