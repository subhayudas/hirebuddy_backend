                    }
                    ).sort( (ot, at) => new Date(at.sent_at).getTime() - new Date(ot.sent_at).getTime())
                      , nt = it.length > 0 ? it[0].sent_at : ""
                      , lt = st.length > 0 ? st[0].sent_at : "";
