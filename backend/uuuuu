            a[d] !== void 0) {
                const h = i[d];
                h === NUMBER_TOKEN ? c += sanitize(a[d]) : h === COLOR_TOKEN ? c += color.transform(a[d]) : c += a[d]
            }
        return c
    }
}
const convertNumbersToZero = s => typeof s == "number" ? 0 : s;
