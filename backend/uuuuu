                    Nt || (bt.width = `${(100 * parseFloat(Yt) / xt).toFixed(2)}%`),
                    !mt(this, tt) && !jt && (bt.height = `${(100 * parseFloat(Tt) / Et).toFixed(2)}%`)
                }
                getInitialTranslation() {
                    return [0, 0]
                }
                async addAltTextButton() {
                    if (mt(this, _))
