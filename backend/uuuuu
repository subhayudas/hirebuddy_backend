        duration: a,
        easing: Array.isArray($) ? "linear" : $,
        fill: "both",
        iterations: c + 1,
        direction: d === "reverse" ? "alternate" : "normal"
    };
    return b && (_e.pseudoElement = b),
    s.animate(_, _e)
}
function isGenerator(s) {
    return typeof s == "function" && "applyToOptions"in s
}
